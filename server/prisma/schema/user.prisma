model User {
  id    String @id @default(uuid()) @db.Uuid
  slug  String @unique
  email String @unique

  display_name String?

  role String @default("default")

  password String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  reviews     FigureTagReview[]
  figure_tags FigureTag[]
  tags        Tag[]
}
