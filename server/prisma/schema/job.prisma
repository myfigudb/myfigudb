enum JobStatus {
  PENDING
  PROCESSING
  DONE
  FAILED

  @@map("job_status")
}

model UrlQueue {
  id  Int    @id @default(autoincrement())
  url String @unique

  status JobStatus @default(PENDING)

  attempts Int     @default(0)
  error    String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([status])
  @@map("url_queue")
}
